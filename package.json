{
  "name": "react-link-text",
  "author": "Ananto Ghosh",
  "version": "0.1.0",
  "description": "A small react package that turns urls in your text clickable (aka linkify).",
  "license": "MIT",
  "type": "module",
  "source": "src/index.tsx",
  "main": "./dist/react-link-text.cjs",
  "module": "./dist/react-link-text.module.js",
  "unpkg": "./dist/react-link-text.umd.js",
  "exports": "./dist/react-link-text.modern.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anantoghosh/react-link-text.git"
  },
  "bugs": {
    "url": "https://github.com/anantoghosh/react-link-text/issues"
  },
  "homepage": "https://github.com/anantoghosh/react-link-text",
  "scripts": {
    "release": "np",
    "build": "microbundle && npm run build:legacy",
    "build:legacy": "cross-env BROWSERSLIST_ENV=legacy microbundle -o dist/legacy -f umd",
    "dev": "microbundle watch",
    "test": "jest"
  },
  "keywords": [
    "react-linkify",
    "react",
    "linkify",
    "link",
    "url",
    "autolink",
    "text",
    "tag"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "cross-env": "^7.0.3",
    "jest": "^27.0.5",
    "microbundle": "^0.13.3",
    "np": "^7.5.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.3"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
